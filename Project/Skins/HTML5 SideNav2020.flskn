          Name="Navigation Panel">
          <Properties>
            <Property
              Name="BackgroundColor">#f9f9f9</Property>
          </Properties>
        </Style>
        <Style
          Name="Navigation Panel Item (hover)">
          <Properties>
            <Property
              Name="BackgroundColor">transparent</Property>
            <Property
              Name="Color">#ee8922</Property>
          </Properties>
