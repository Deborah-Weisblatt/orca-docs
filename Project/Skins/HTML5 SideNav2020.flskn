            <StyleClass
              Name="Expander Icon">
              <Properties>
                <Property
                  Name="ExpanderBorderWidth">6px</Property>
              </Properties>
            </StyleClass>
          </Classes>
        </Style>
        <Style
          Name="Navigation Panel Item">
          <Classes>
            <StyleClass
              Name="Expander Icon">
              <Properties>
                <Property
                  Name="ExpanderBorderColor">#b9b9b9</Property>
              </Properties>
            </StyleClass>
          </Classes>
          <Properties>
            <Property
              Name="FontFamily">'futura-pt-book', sans-serif</Property>
            <Property
              Name="FontSize">15px</Property>
            <Property
              Name="BorderBottom">solid 1px #dddddd</Property>
            <Property
              Name="Color">#333333</Property>
          </Properties>
        </Style>
        <Style
          Name="Search Bar">
          <Properties>
            <Property
              Name="Stretch">no</Property>
            <Property
              Name="MarginTop">0px</Property>
            <Property
              Name="Width">336px</Property>
            <Property
              Name="AlignmentHorizontal">right</Property>
          </Properties>
        </Style>
        <Style
          Name="Logo">
          <Properties>
            <Property
              Name="Stretch">no</Property>
          </Properties>
        </Style>
        <Style
          Name="Navigation Slide Out Button">
          <Properties>
            <Property
              Name="BackgroundColor">#ec7700</Property>
          </Properties>
        </Style>
        <Style
          Name="Navigation Panel Item (selected)">
          <Properties>
            <Property
              Name="Color">#555555</Property>
          </Properties>
        </Style>
        <Style
          Name="Header">
          <Properties>
            <Property
              Name="BackgroundColor">#ffffff</Property>
          </Properties>
        </Style>
      </Medium>
      <Style
        Name="Side Navigation Menu Item (selected)">
        <Classes>
          <StyleClass
            Name="1st Level">
            <Properties>
              <Property
                Name="Color">#EC7700</Property>
              <Property
                Name="FontWeight">normal</Property>
              <Property
                Name="LineHeight">18px</Property>
            </Properties>
          </StyleClass>
        </Classes>
      </Style>
    </Styles>
    <Stylesheet></Stylesheet>
  </Stylesheet>
  <Strings>
    <Stylesheet
      lang="en-us">
      <Stylesheet>
        <Styles>
          <Style
            Name="Search Filter">
            <Properties>
              <Property
                Name="Label">All</Property>
            </Properties>
          </Style>
          <Style
            Name="Search Input">
            <Properties>
              <Property
                Name="Placeholder">Looking for something?</Property>
            </Properties>
          </Style>
        </Styles>
      </Stylesheet>
    </Stylesheet>
  </Strings>
</CatapultSkin>